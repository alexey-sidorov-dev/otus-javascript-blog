{
  "name": "otus-javascript-blog",
  "version": "1.0.0",
  "main": "./src/index.js",
  "description": "OTUS JavaScript Blog",
  "scripts": {
    "test": "jest",
    "test:coverage": "jest --coverage",
    "lint": "eslint . --config eslint.config.js",
    "lint:fix": "eslint . --config eslint.config.js --fix",
    "lint:all": "eslint . --config eslint.config.js && stylelint **/*.{css,scss} && prettier . --check",
    "lint:all:fix": "eslint . --config eslint.config.js --fix && stylelint **/*.{css,scss} --fix && prettier . --write",
    "prettier": "prettier . --check",
    "prettier:fix": "prettier . --write",
    "stylelint": "stylelint **/*.{css,scss}",
    "stylelint:fix": "stylelint **/*.{css,scss} --fix",
    "prepare": "husky install",
    "build": "webpack build --node-env production --config webpack.prod.js",
    "start": "webpack serve --config webpack.dev.js --mode development",
    "dev": "webpack serve --config webpack.dev.js --open"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alexey-sidorov-dev/otus-javascript-blog.git"
  },
  "keywords": [
    "javascript"
  ],
  "author": "Alexey Sidorov",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/alexey-sidorov-dev/otus-javascript-blog/issues"
  },
  "homepage": "https://github.com/alexey-sidorov-dev/otus-javascript-blog#readme",
  "devDependencies": {
    "@babel/cli": "^7.17.10",
    "@babel/core": "^7.17.10",
    "@babel/plugin-transform-runtime": "^7.17.10",
    "@babel/preset-env": "^7.17.10",
    "@types/jest": "^27.4.1",
    "babel-jest": "^28.0.3",
    "babel-loader": "^8.2.5",
    "browser-sync-webpack-plugin": "^2.3.0",
    "copy-webpack-plugin": "^10.2.4",
    "core-js": "^3.22.3",
    "css-loader": "^6.7.1",
    "css-minimizer-webpack-plugin": "^3.4.1",
    "eslint": "^8.14.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^26.1.5",
    "fast-glob": "^3.2.11",
    "handlebars": "^4.7.7",
    "handlebars-loader": "^1.7.1",
    "html-loader": "^3.1.0",
    "html-webpack-plugin": "^5.5.0",
    "husky": "^7.0.4",
    "jest": "^28.0.3",
    "lint-staged": "^12.4.1",
    "mini-css-extract-plugin": "^2.6.0",
    "postcss": "^8.4.13",
    "postcss-import": "^14.1.0",
    "postcss-loader": "^6.2.1",
    "postcss-preset-env": "^7.4.4",
    "prettier": "^2.6.2",
    "sass": "^1.51.0",
    "sass-loader": "^12.6.0",
    "style-loader": "^3.3.1",
    "stylelint": "^14.8.1",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-prettier-scss": "^0.0.1",
    "stylelint-config-standard": "^25.0.0",
    "stylelint-config-standard-scss": "^3.0.0",
    "webpack": "^5.72.0",
    "webpack-bundle-analyzer": "^4.5.0",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.8.1",
    "webpack-merge": "^5.8.0"
  },
  "dependencies": {
    "modern-css-reset": "^1.4.0"
  },
  "overrides": {
    "browser-sync-webpack-plugin": {
      "browser-sync": {
        "portscanner": {
          "async": "2.6.4"
        }
      }
    },
    "handlebars-loader": {
      "async": "2.6.4"
    }
  }
}
